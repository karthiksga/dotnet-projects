<UserControl x:Class="SilverlightControls.ControlStyle"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    xmlns:control="clr-namespace:SilverlightControls"
    d:DesignHeight="300" d:DesignWidth="400">
	<UserControl.Resources>
		<Style x:Key="ContentRootStyle" TargetType="Grid"/>
	</UserControl.Resources>
    
    <Grid x:Name="ContentRoot_Part" Style="{StaticResource ContentRootStyle}">
    	<Canvas x:Name="Canvas_Part"></Canvas>   		 	
	</Grid>
	

	
</UserControl>
