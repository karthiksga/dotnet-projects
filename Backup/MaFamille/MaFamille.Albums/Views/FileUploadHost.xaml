<UserControl x:Class="MaFamille.Albums.Views.FileUploadHost"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:fw="clr-namespace:SilverlightControls;assembly=SilverlightControls"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400">
    
    <Grid x:Name="LayoutRoot">
    	<Grid.Resources>
    		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="/SilverlightControls;component/Themes/generic.xaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>  	
		</Grid.Resources>
        <fw:WindowHost Name="windowHost">
            <fw:FloatWindow></fw:FloatWindow>
        </fw:WindowHost>
    </Grid>
</UserControl>
